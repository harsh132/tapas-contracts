// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Accounts {
  id           Int      @id @default(autoincrement())
  chain        String
  smartAccount String
  owner        String
  signer       String

  @@index([chain, smartAccount])
}
